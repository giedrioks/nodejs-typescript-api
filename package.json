{
  "name": "nodejs-typescript-api",
  "version": "1.0.0",
  "description": "This is sample API project with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src -e ts --exec ts-node -r dotenv/config src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -r dist && rm -r node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "Giedrius Valintelis",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "node-fetch": "^2.6.7",
    "pino": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "^17.0.35",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@types/jest": "^28.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.4"
  }
}
